{
  "name": "pokemon",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "pokemon/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/pokemon",
        "index": "pokemon/src/index.html",
        "baseHref": "/",
        "main": "pokemon/src/main.ts",
        "tsConfig": "pokemon/tsconfig.app.json",
        "assets": ["pokemon/src/favicon.ico", "pokemon/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "pokemon/webpack.config.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "pokemon/src/environments/environment.ts",
              "with": "pokemon/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "pokemon/webpack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "pokemon:build",
        "hmr": true,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "pokemon:build:development"
        },
        "production": {
          "buildTarget": "pokemon:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "pokemon:build",
        "watch": false,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "pokemon:build:development"
        },
        "production": {
          "buildTarget": "pokemon:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "pokemon/jest.config.ts"
      }
    }
  },
  "tags": []
}
