{
  "name": "list-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "pokemon",
  "sourceRoot": "apps/list-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/list-app",
        "index": "apps/list-app/src/index.html",
        "main": "apps/list-app/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/list-app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/list-app/src/favicon.ico", "apps/list-app/src/assets"],
        "styles": [
          "apps/list-app/src/styles.scss",
          "node_modules/ng-zorro-antd/ng-zorro-antd.min.css"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/list-app/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/list-app/webpack.prod.config.ts"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "port": 4201
      },
      "configurations": {
        "production": {
          "buildTarget": "list-app:build:production"
        },
        "development": {
          "buildTarget": "list-app:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "list-app:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/list-app/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "list-app:build",
        "port": 4201,
        "watch": false
      },
      "configurations": {
        "development": {
          "buildTarget": "list-app:build:development"
        },
        "production": {
          "buildTarget": "list-app:build:production"
        }
      }
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/list-app/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "list-app:build"
      }
    }
  }
}
