{
  "name": "details-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/details-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/details-app",
        "index": "apps/details-app/src/index.html",
        "baseHref": "/",
        "main": "apps/details-app/src/main.ts",
        "tsConfig": "apps/details-app/tsconfig.app.json",
        "assets": [
          "apps/details-app/src/favicon.ico",
          "apps/details-app/src/assets"
        ],
        "styles": ["apps/details-app/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "apps/details-app/webpack.config.serve.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/details-app/src/environments/environment.ts",
              "with": "apps/details-app/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "apps/details-app/webpack.config.prod.ts"
        }
      }
    },
    "build-mfe": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/details-app",
        "index": "apps/details-app/src/index.html",
        "baseHref": "/",
        "main": "apps/details-app/src/main.ts",
        "tsConfig": "apps/details-app/tsconfig.app.json",
        "assets": [
          "apps/details-app/src/favicon.ico",
          "apps/details-app/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/details-app/webpack.config.mfe.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "buildTarget": "details-app:build:production"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "details-app:build",
        "hmr": true,
        "port": 4202
      },
      "configurations": {
        "development": {
          "buildTarget": "details-app:build:development"
        },
        "production": {
          "buildTarget": "details-app:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "details-app:build-mfe",
        "watch": false,
        "port": 4202
      },
      "configurations": {
        "development": {
          "buildTarget": "details-app:build-mfe:development"
        },
        "production": {
          "buildTarget": "details-app:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/details-app/jest.config.ts"
      }
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "tsConfig": "apps/details-app/tsconfig.json",
        "cypressConfig": "apps/details-app/cypress.config.ts",
        "testingType": "component",
        "devServerTarget": "details-app:build",
        "skipServe": true
      }
    }
  },
  "tags": []
}
